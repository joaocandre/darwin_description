  <!-- Left Leg -->
  <link name="Left_FSR_FL_frame">
    <inertial>
      <!-- reference: darwin webots proto file (.vrml) 'mass' field -->
      <mass value="0.08057"/>      
      <!-- reference: generic rotational inertial tensor of cylindrical shapes -->
      <!-- specific inertial reference frame is kept the same as the link reference frame -->
      <inertia ixx="1.343e-06" ixy="0" ixz="0" iyy="1.343e-06" iyz="0" izz="1.343e-06"/>
      <!-- <inertia ixx="0.671e-06" ixy="0" ixz="0" iyy="0.671e-06" iyz="0" izz="1.007e-06"/> -->
      <!-- inertial matrix from NAO fsr sensors: -->
      <!-- <inertia ixx="1.1e-09" ixy="0" ixz="0" iyy="1.1e-09" iyz="0" izz="1.1e-09"/> -->
    </inertial>
    <!-- <visual> properties to help debug sensor position -->
    <!-- joint origin is the point of contact on the foot sole -->
    <!-- need to rotate and displace cylinder to place it within the foot -->
    <visual>
      <origin rpy="0 0 0"  xyz="0.005 0 0"/>
      <!-- <origin rpy="3.141592654 1.570796327 0"  xyz="0.0025 0 0"/> -->
      <geometry>
        <!-- webots model uses a boundingBox around the sensor -->
        <box size="0.01 0.01 0.01"/>
        <!-- <cylinder radius="0.005" length="0.005" /> -->
      </geometry>
    </visual>
    <!-- <collision> properties to define how it interacts with other objects -->
    <!-- should not be necessary as the sensor is inside the foot -->
    <!-- as long as mass is correctly defined, shape should only influence sensor readings -->
    <collision>
      <origin rpy="0 0 0"  xyz="0.005 0 0"/>
      <!-- <origin rpy="3.141592654 1.570796327 0"  xyz="0.0025 0 0"/> -->
      <geometry>
        <!-- reference: darwin webots proto file (.vrml) 'bounding box' field -->
        <!-- despite webots using a cube to model the sensor, its shape is circular/cylinder -->
        <box size="0.01 0.01 0.01"/>
        <!-- <cylinder radius="0.005" length="0.005" /> -->
      </geometry>
    </collision>
  </link>

